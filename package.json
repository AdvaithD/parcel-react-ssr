{
  "name": "parcel-react-ssr",
  "version": "0.2.0",
  "description": "Example of SSR with React and ParcelJS",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf dist && npm run build-client && npm run build-server",
    "build-client": "parcel build src/index.html -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=node babel src -d dist/server",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": "reactivestack/parcel-react-ssr",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest": "^21.2.1",
    "parcel-bundler": "^1.2.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.2",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "whatwg-fetch": "^2.0.3"
  }
}
