{
  "name": "parcel-react-ssr",
  "version": "0.1.0",
  "description": "Example of SSR with React and ParcelJS",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf dist && npm run build-client && npm run build-server",
    "build-client": "parcel build src/index.html --no-minify -d dist/client --public-url /dist",
    "build-server": "cross-env BABEL_ENV=node babel src -d dist/server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "parcel-bundler": "^1.0.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
